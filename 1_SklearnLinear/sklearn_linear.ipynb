    "import numpy as np\n",
    "import pandas as pd\n",
    "from sklearn.linear_model import LinearRegression\n",
    "\n",
    "df = pd.read_csv('Advertising.csv')\n",
    "\n",
    "X = df[['TV']].values\n",
    "y = df[['Sales']].values\n",
    "\n",
    "reg = LinearRegression()\n",
    "reg.fit(X,y)\n",
    "reg.coef_\n",
    "reg.intercept_\n",
    "reg.predict(np.array([[100]]))\n",
    "\n",
    "\n",
    "\n"
   ]
  }
